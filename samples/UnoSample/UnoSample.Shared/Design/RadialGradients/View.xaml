<UserControl
    x:Class="UWPSample.Design.RadialGradients.View"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Uno"
    xmlns:vms="using:ViewModelsSamples.Design.RadialGradients"
    mc:Ignorable="d">

    <UserControl.DataContext>
        <vms:ViewModel/>
    </UserControl.DataContext>
    <Grid>
        <lvc:PieChart Series="{Binding Series}" LegendPosition="Right"></lvc:PieChart>
    </Grid>
    
</UserControl>
